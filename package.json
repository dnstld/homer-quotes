{
  "name": "HomerQuotes",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "APP_VARIANT=development expo start",
    "prod": "npx expo start --no-dev --minify",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eas:login": "eas login",
    "build:ios": "eas build --platform ios --profile ios-simulator",
    "build:dev:all": "eas build --platform all --profile development",
    "build:dev:android": "eas build --platform android --profile development",
    "build:dev:ios": "eas build --platform ios --profile development",
    "build:prod:all": "eas build --platform all",
    "build:prod:ios": "eas build --platform ios",
    "build:prod:android": "eas build --platform android",
    "build:update:prod": "eas update --channel production",
    "submit:ios": "eas submit -p ios --latest"
  },
  "dependencies": {
    "@expo-google-fonts/acme": "^0.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^20.5.0",
    "@sentry/react-native": "~5.22.0",
    "expo": "~51.0.31",
    "expo-application": "~5.9.1",
    "expo-asset": "~10.0.10",
    "expo-build-properties": "~0.12.5",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.25",
    "expo-device": "~6.0.2",
    "expo-intent-launcher": "~11.0.1",
    "expo-linking": "~6.3.1",
    "expo-navigation-bar": "~3.0.7",
    "expo-notifications": "~0.28.16",
    "expo-router": "~3.5.23",
    "expo-sharing": "^12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.24",
    "firebase": "^10.12.4",
    "qs": "^6.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-auto-size-text": "^1.1.1",
    "react-native-flash-message": "^0.4.2",
